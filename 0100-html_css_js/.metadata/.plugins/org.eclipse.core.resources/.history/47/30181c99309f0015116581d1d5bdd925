<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>函数</title>
</head>
<script type="text/javascript">
	console.log(typeof name);
	fun1(name);
	var name=null;
	fun1(name);
	console.log(typeof name);
	function fun1(name){
		//注意运算符 ==和===
		if(typeof name === null){
			console.log("名字是null，请重新输入");
		}else if(typeof name === undefined){
			console.log("名字是undefined，请重新输入");
		}else if(typeof name == 'boolean'){
			console.log("名字是布尔型，请重新输入");
		}else if(typeof name == 'number'){
			console.log("名字是数字，请重新输入");
		}else if(typeof name === 'object'){
			console.log("名字是对象型，请重新输入");
		}else if(typeof name == 'string'){
			console.log("输入正确，名字是"+name);
		}
	}
	
	
</script>
</html>